    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>7fa4ba4d-f430-47fa-8a22-cfbc32215961</UserSecretsId>
    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="9.0.9">
